{
  "name": "boot-ts",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@10.12.4",
  "description": "_description_",
  "author": "Kirk Lin <https://github.com/kirklin>",
  "license": "MIT",
  "funding": "https://www.buymeacoffee.com/linkirk",
  "homepage": "https://github.com/kirklin/boot-ts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kirklin/boot-ts.git"
  },
  "bugs": "https://github.com/kirklin/boot-ts/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "npm run build",
    "release": "bumpp",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks",
    "up": "taze latest -r -w -I"
  },
  "dependencies": {
    "@kirklin/logger": "catalog:dev"
  },
  "devDependencies": {
    "@antfu/ni": "catalog:cli",
    "@kirklin/eslint-config": "catalog:cli",
    "@types/node": "catalog:types",
    "bumpp": "catalog:cli",
    "eslint": "catalog:cli",
    "esno": "catalog:cli",
    "lint-staged": "catalog:cli",
    "pnpm": "catalog:cli",
    "rimraf": "catalog:cli",
    "simple-git-hooks": "catalog:cli",
    "taze": "catalog:cli",
    "typescript": "catalog:cli",
    "unbuild": "catalog:cli",
    "vite": "catalog:cli",
    "vitest": "catalog:testing"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm i --frozen-lockfile --ignore-scripts --offline && npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
